{
  "name": "awdware-hosting",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "author": "Janik Schumacher <rndmb0t@gmail.com>",
  "license": "MIT",
  "prettier": "@awdware/prettier-config",
  "scripts": {
    "generate": "yarn ts-node --esm ./src/index.ts",
    "pull": "yarn ts-node --esm ./output/git-pull.ts",
    "secrets": "yarn ts-node --esm ./output/copy-secrets.ts",
    "docker": "docker compose -f ./output/docker-compose.yml up -d --force-recreate",
    "deploy-test": "yarn pull test && yarn generate && yarn secrets test && yarn docker",
    "deploy-prod": "yarn pull prod && yarn generate && yarn secrets prod && yarn docker"
  },
  "devDependencies": {
    "@awdware/prettier-config": "^0.0.2",
    "@types/node": "^17.0.21",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.2"
  },
  "dependencies": {}
}
